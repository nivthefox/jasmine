  "scripts": {
    "ci-test": "NODE_PATH=. ./node_modules/instanbul/lib/cli.js cover ./node_modules/.bin/_mocha --report lcovonly -- --sort --check-leaks $(find tst -name *.tests.js) && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "NODE_PATH=. ./node_modules/instanbul/lib/cli.js cover ./node_modules/.bin/_mocha -- --sort --check-leaks $(find tst -name *.tests.js)",
    "test": "NODE_PATH=. ./node_modules/.bin/mocha --recursive --sort --check-leaks $(find tst -name *.tests.js)",
    "start": "NODE_PATH=. node server.js"
  }
